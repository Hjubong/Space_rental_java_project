	    public static void main(String[] args) {
	        int numberOfNames = 100; // »ý¼ºÇÒ ´õ¹Ì ÀÌ¸§ °³¼ö

	        for (int i = 0; i < numberOfNames; i++) {
	            String name = generateDummyKoreanName();
	            System.out.println(name);
	        }
	    }

	    public static String generateDummyKoreanName() {
	        Random random = new Random();
	        StringBuilder name = new StringBuilder();

	        // ÇÑ±Û ÃÊ¼º ¸®½ºÆ® (°¡~ÆR ¹üÀ§ ³»¿¡¼­)
	        char[] initials = {'¤¡', '¤¢', '¤¤', '¤§', '¤¨', '¤©', '¤±', '¤²', '¤³', '¤µ', '¤¶', '¤·', '¤¸', '¤¹', '¤º', '¤»', '¤¼', '¤½', '¤¾'};

	        // ¹«ÀÛÀ§·Î 3±ÛÀÚ ÇÑ±Û ÀÌ¸§ »ý¼º
	        for (int i = 0; i < 3; i++) {
	            char randomChar = initials[random.nextInt(initials.length)]; // ¹«ÀÛÀ§ ÃÊ¼º ¼±ÅÃ
	            name.append(randomChar);
	        }

	        return name.toString();
	    }
	

	

